{
  "name": "@strav/config",
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": true,
  "scripts": {
    "build": "bun build src/index.ts --no-mangle --outdir=dist --target=bun --format=esm",
    "build:types": "tsc --emitDeclarationOnly --declaration --outDir dist",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "typecheck": "tsc --noEmit"
  },
  "keywords": ["dependency-injection", "di", "bun", "typescript", "strav"],
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/bun": "latest",
    "bun-types": "latest",
    "eslint": "^9.35.0",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "reflect-metadata": "^0.2.2"
  }
}
