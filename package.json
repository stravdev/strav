{
  "name": "strav",
  "private": true,
  "packageManager": "bun@1.2.21",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "bun run --filter='*' build",
    "build:types": "bun run --filter='*' build:types",
    "test": "bun run --filter='*' test",
    "publish-all": "bun run --filter='*' publish",
    "typecheck": "bun run --filter='*' typecheck --noEmit"
  },
  "devDependencies": {
    "bun": "latest"
  }
}