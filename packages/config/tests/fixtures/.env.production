# Production environment configuration
NODE_ENV=production
APP_NAME=ProductionApp
APP_VERSION=2.1.0
APP_DEBUG=false
APP_PORT=8080

# Production database
DATABASE_HOST=prod-db.example.com
DATABASE_PORT=5432
DATABASE_NAME=prod_db
DATABASE_SSL=true

# Production API
API_BASE_URL=https://api.production.com
API_TIMEOUT=10000
API_RETRY_COUNT=5

# Production cache
REDIS_HOST=prod-redis.example.com
REDIS_PORT=6379
REDIS_TTL=7200

# Production logging
LOG_LEVEL=info
LOG_FORMAT=structured
LOG_OUTPUT=file